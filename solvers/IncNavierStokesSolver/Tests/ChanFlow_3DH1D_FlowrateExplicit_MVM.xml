            </REGION>
            <REGION REF="3">
                <D VAR="u" VALUE="0" />
                <D VAR="v" VALUE="0" />
                <D VAR="w" VALUE="0" />
                <N VAR="p" USERDEFINEDTYPE="H" VALUE="0" />  // High Order Pressure BC
            </REGION>            
        </BOUNDARYCONDITIONS>
        
        
        <FUNCTION NAME="FlowrateForce">
            <E VAR="ForceX" VALUE="1" />
            <E VAR="ForceY" VALUE="0" />
            <E VAR="ForceZ" VALUE="0" />
        </FUNCTION>
        
        <FUNCTION NAME="InitialConditions">
        	<E VAR="u" VALUE="0" />
        	<E VAR="v" VALUE="0" />
        	<E VAR="w" VALUE="0" />
        	<E VAR="p" VALUE="0" />        	       
		</FUNCTION>
	
	        <FUNCTION NAME="ExactSolution">
            <E VAR="u" VALUE="6*y*(1-y)" />
            <E VAR="v" VALUE="0" />
            <E VAR="w" VALUE="0" />            
            <E VAR="p" VALUE="0" />
        </FUNCTION>
    </CONDITIONS>    
       

