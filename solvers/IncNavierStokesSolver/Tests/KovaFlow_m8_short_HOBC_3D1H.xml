<?xml version="1.0" encoding="utf-8" ?>

<NEKTAR xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.nektar.info/schema/nektar.xsd">

    <EXPANSIONS>
        <E COMPOSITE="C[0]" NUMMODES="8" FIELDS="u,v,w,p" TYPE="MODIFIED" />
    </EXPANSIONS>

    <CONDITIONS>
        <SOLVERINFO>
            <I PROPERTY="SolverType" VALUE="VelocityCorrectionScheme" />
            <I PROPERTY="EQTYPE" VALUE="UnsteadyNavierStokes" />
            <I PROPERTY="AdvectionForm" VALUE="Convective" />
            <I PROPERTY="Projection" VALUE="Galerkin" />
            <I PROPERTY="TimeIntegrationMethod" VALUE="IMEXOrder2" />
            <I PROPERTY="HOMOGENEOUS" VALUE="1D"/>
            <I PROPERTY="USEFFT" VALUE="FFTW"/>
	    <I PROPERTY="DEALIASING" VALUE="True"/>
        </SOLVERINFO>

        <PARAMETERS>
          <P> TimeStep      = 0.001   </P>
          <P> NumSteps      = 10     </P>
          <P> IO_CheckSteps = 10      </P>
          <P> IO_CFLSteps   = 100     </P>
          <P> IO_InfoSteps  = 1       </P>
          <P> Kinvis        = 0.025   </P>
          <P> KovLam        = 0.5/Kinvis-sqrt(0.25/(Kinvis*Kinvis)+4*PI*PI) </P>
          <P> HomModesZ     = 32       </P>
          <P> LZ            = 1       </P>
          <P> PROC_Z        = 1       </P>
        </PARAMETERS>

        <VARIABLES>
          <V ID="0"> u </V>
          <V ID="1"> v </V>
          <V ID="2"> w </V>
          <V ID="3"> p </V>
        </VARIABLES>

        <BOUNDARYREGIONS>
          <B ID="0"> C[1] </B>
          <B ID="1"> C[2] </B>
          <B ID="2"> C[3] </B>
        </BOUNDARYREGIONS>

        <BOUNDARYCONDITIONS>
          <REGION REF="0">
            <D VAR="u" VALUE="(1-exp(KovLam*x)*cos(2*PI*z))" />
            <D VAR="v" VALUE="0" />
            <D VAR="w" VALUE="KovLam/(2*PI)*exp(KovLam*x)*sin(2*PI*z)" />
            <N VAR="p" USERDEFINEDTYPE="H" VALUE="0"          />
          </REGION>
          <REGION REF="1">
            <N VAR="u" USERDEFINEDTYPE="HOutflow" VALUE="Kinvis*KovLam*exp(KovLam*x)*cos(2*PI*z) + 0.5*(1-exp(2*KovLam*x))+0.5*(((1-exp(KovLam*x)*cos(2*PI*z))*(1-exp(KovLam*x)*cos(2*PI*z))+(KovLam/(2*PI)*exp(KovLam*x)*sin(2*PI*z))*(KovLam/(2*PI)*exp(KovLam*x)*sin(2*PI*z))))*(0.5*(1.0-tanh((1-exp(KovLam*x)*cos(2*PI*z))*20)))" />
            <N VAR="v" VALUE="0" />
            <N VAR="w" USERDEFINEDTYPE="HOutflow" VALUE="-Kinvis*KovLam*KovLam/(2*PI)*exp(KovLam*x)*sin(2*PI*z)" />
            <D VAR="p" USERDEFINEDTYPE="HOutflow" VALUE="0" />
          </REGION>
          <REGION REF="2">
            <N VAR="u" VALUE="0" />
            <D VAR="v" VALUE="0" />
            <N VAR="w" VALUE="0" />
            <N VAR="p" VALUE="0" />
          </REGION>
        </BOUNDARYCONDITIONS>        
        
        <FUNCTION NAME="InitialConditions">
          <E VAR="u" VALUE="(1-exp(KovLam*x)*cos(2*PI*z))" />
          <E VAR="v" VALUE="0" />
          <E VAR="w" VALUE="(KovLam/(2*PI))*exp(KovLam*x)*sin(2*PI*z)" />
          <E VAR="p" VALUE="0.5*(1-exp(2*KovLam*x))" />
        </FUNCTION>
        
        <FUNCTION NAME="ExactSolution">
          <E VAR="u" VALUE="(1-exp(KovLam*x)*cos(2*PI*z))" />
          <E VAR="v" VALUE="0" />
		  <E VAR="w" VALUE="(KovLam/(2*PI))*exp(KovLam*x)*sin(2*PI*z)" />
          <E VAR="p" VALUE="0.5*(1-exp(2*KovLam*x))" />
        </FUNCTION>
        
    </CONDITIONS>
    
    <GEOMETRY DIM="2" SPACE="2">

      <VERTEX>
            <V ID="0">-5.000e-01  -5.000e-01 0.000e+00</V>
            <V ID="1">-5.000e-01   5.000e-01 0.000e+00</V>
            <V ID="2">-1.000e-01   5.000e-01 0.000e+00</V>
            <V ID="3">-1.000e-01  -5.000e-01 0.000e+00</V>
            <V ID="4">-5.000e-01   2.500e-01 0.000e+00</V>
            <V ID="5">-5.000e-01   0.000e-01 0.000e+00</V>
            <V ID="6">-5.000e-01  -2.500e-01 0.000e+00</V>
            <V ID="7">-3.666e-01  -5.000e-01 0.000e+00</V>
            <V ID="8">-2.333e-01  -5.000e-01 0.000e+00</V>
            <V ID="9">-1.000e-01  -2.500e-01 0.000e+00</V>
            <V ID="10">-1.000e-01  0.000e-01 0.000e+00</V>
            <V ID="11">-1.000e-01  2.500e-01 0.000e+00</V>
            <V ID="12">-2.333e-01  5.000e-01 0.000e+00</V>
            <V ID="13">-3.666e-01  5.000e-01 0.000e+00</V>
            <V ID="14">-2.333e-01  2.500e-01 0.000e+00</V>
            <V ID="15">-2.333e-01  0.000e-01 0.000e+00</V>
            <V ID="16">-2.333e-01 -2.500e-01 0.000e+00</V>
            <V ID="17">-3.666e-01  2.500e-01 0.000e+00</V>
            <V ID="18">-3.666e-01  0.000e-01 0.000e+00</V>
            <V ID="19">-3.666e-01 -2.500e-01 0.000e+00</V>
        </VERTEX>

        <EDGE>
            <E ID="0">    2  12   </E>
            <E ID="1">   12  14   </E>
            <E ID="2">   14  11   </E>
            <E ID="3">   11  2   </E>
            <E ID="4">   14  15   </E>
            <E ID="5">   15  10   </E>
            <E ID="6">   10  11   </E>
            <E ID="7">   15  16   </E>
            <E ID="8">   16  9   </E>
            <E ID="9">    9  10   </E>
            <E ID="10">   16  8   </E>
            <E ID="11">    8  3   </E>
            <E ID="12">    3  9   </E>
            <E ID="13">   12  13   </E>
            <E ID="14">   13  17   </E>
            <E ID="15">   17  14   </E>
            <E ID="16">   17  18   </E>
            <E ID="17">   18  15   </E>
            <E ID="18">   18  19   </E>
            <E ID="19">   19  16   </E>
            <E ID="20">   19  7   </E>
            <E ID="21">    7  8   </E>
            <E ID="22">   13  1   </E>
            <E ID="23">    1  4   </E>
            <E ID="24">    4  17   </E>
            <E ID="25">    4  5   </E>
            <E ID="26">    5  18   </E>
            <E ID="27">    5  6   </E>
            <E ID="28">    6  19   </E>
            <E ID="29">    6  0   </E>
            <E ID="30">    0  7   </E>
        </EDGE>

        <ELEMENT>
            <Q ID="0">    0     1     2     3 </Q>
            <Q ID="1">    2     4     5     6 </Q>
            <Q ID="2">    5     7     8     9 </Q>
            <Q ID="3">    8    10    11    12 </Q>
            <Q ID="4">   13    14    15     1 </Q>
            <Q ID="5">   15    16    17     4 </Q>
            <Q ID="6">   17    18    19     7 </Q>
            <Q ID="7">   19    20    21    10 </Q>
            <Q ID="8">   22    23    24    14 </Q>
            <Q ID="9">   24    25    26    16 </Q>
            <Q ID="10">   26    27    28    18 </Q>
            <Q ID="11">   28    29    30    20 </Q>
        </ELEMENT>

        <COMPOSITE>
            <C ID="0"> Q[0-11]             </C>     <!-- Domain -->
            <C ID="1"> E[23,25,27,29]      </C>     <!-- Inflow -->
            <C ID="2"> E[3,6,9,12]         </C>     <!-- Outflow -->
            <C ID="3"> E[0,11,13,21,22,30] </C>     <!-- Walls -->
        </COMPOSITE>

        <DOMAIN> C[0] </DOMAIN>

    </GEOMETRY>

</NEKTAR>
    
