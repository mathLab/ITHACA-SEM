            <T ID="1">         0         5         4</T>
            <T ID="2">         1         6         5</T>
            <T ID="3">         2         6         7</T>
            <T ID="4">         3         7         4</T>
            <Q ID="5">         8         9        10         0</Q>
            <T ID="6">         8        11         4</T>
