        <BOUNDARYCONDITIONS>
            <REGION REF="0">    <!-- Walls -->
                <D VAR="u" VALUE="0" />
                <D VAR="v" VALUE="0" />
            </REGION>
            <REGION REF="1">    <!-- InFlow -->
                <!-- <D VAR="u" VALUE="(y*(1-y))" /> -->
                <D VAR="u" VALUE="( (y-2.5)*(1.0-(y-2.5)) )" />
                <D VAR="v" VALUE="0" />
            </REGION>
            <REGION REF="2">    <!-- OutFlow -->
                <N VAR="u" VALUE="0" />
                <N VAR="v" VALUE="0" />
            </REGION>
        </BOUNDARYCONDITIONS>

        <FUNCTION NAME="InitialConditions">
          <!--     <E VAR="u" VALUE="0" />
            <E VAR="v" VALUE="0" /> -->
        </FUNCTION> 

        <FUNCTION NAME="AdvectionVelocity">
           <E VAR="u" VALUE="0" />
