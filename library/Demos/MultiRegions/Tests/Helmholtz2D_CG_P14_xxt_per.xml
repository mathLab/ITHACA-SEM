<?xml version="1.0" encoding="utf-8" ?>
<NEKTAR>
    <EXPANSIONS>
        <E COMPOSITE="C[0,1]" NUMMODES="15" TYPE="MODIFIED" FIELDS="u" />
    </EXPANSIONS>
    <CONDITIONS>
        <SOLVERINFO>
            <I PROPERTY="EQTYPE" VALUE="Helmholtz" />
            <I PROPERTY="Projection" VALUE="Continuous" />
            <I PROPERTY="Preconditioner" VALUE="Diagonal" />
            <I PROPERTY="GlobalSysSoln" VALUE="XxtFull" />
        </SOLVERINFO>

        <PARAMETERS>
            <P> Lambda = 1 </P> 
            <P> IterativeSolverTolerance = 1e-8 </P> 
        </PARAMETERS>
        
        <VARIABLES>
            <V ID="0"> u </V>
        </VARIABLES>
        
        <BOUNDARYREGIONS>
            <B ID="0"> C[2] </B>
            <B ID="1"> C[3] </B>
            <B ID="2"> C[4] </B>
            <B ID="3"> C[5] </B>
        </BOUNDARYREGIONS>
        
        <BOUNDARYCONDITIONS>
            <REGION REF="0">
                <!--<D VAR="u" VALUE="cos(PI*x)*cos(PI*y)" />-->
                <P VAR="u" VALUE="[3]" />
            </REGION>
            <REGION REF="1">
                <D VAR="u" VALUE="cos(PI*x)*cos(PI*y)" />
            </REGION>
            <REGION REF="2">
                <D VAR="u" VALUE="cos(PI*x)*cos(PI*y)" />
            </REGION>
            <REGION REF="3">
                <!--<D VAR="u" VALUE="cos(PI*x)*cos(PI*y)" />-->
                <P VAR="u" VALUE="[0]" />
            </REGION>
        </BOUNDARYCONDITIONS>
        
        <FUNCTION NAME="Forcing">
            <E VAR="u" VALUE="-(Lambda+2*PI*PI)*cos(PI*x)*cos(PI*y)" />
        </FUNCTION>

        <FUNCTION NAME="ExactSolution">
            <E VAR="u" VALUE="cos(PI*x)*cos(PI*y)" />
        </FUNCTION>
    </CONDITIONS>
