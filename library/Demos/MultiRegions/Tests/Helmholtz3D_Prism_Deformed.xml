      <E ID="16"> 9 10 </E>
      <E ID="17"> 10 5 </E>
      
      <E ID="18"> 1 9 </E>
      <E ID="19"> 2 10 </E>
    </EDGE>
    <FACE>
      <Q ID="0">  3 0 1 2    </Q>
      <T ID="1">  0 5 4      </T>
      <Q ID="2">  1 6 8 5    </Q>
      <T ID="3">  2 6 7      </T>
