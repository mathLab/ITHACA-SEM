<?xml version="1.0" encoding="utf-8"?>

<NEKTAR xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.nektar.info/schema/nektar.xsd">

    <EXPANSIONS>
        <E COMPOSITE="C[0]" NUMMODES="4" FIELDS="u,v" TYPE="MODIFIED" />
	<!--        <E COMPOSITE="C[0]" NUMMODES="7" FIELDS="u,v,p" TYPE="MODIFIED" />     -->
    </EXPANSIONS>

    <CONDITIONS>
        <SOLVERINFO>
            <!-- <I PROPERTY="SolverType" VALUE="CoupledLinearisedNS" /> -->
            <!-- <I PROPERTY="SolverType" VALUE="CoupledLinearisedNS" />  -->
            <!-- <I PROPERTY="MappingImplicitPressure" VALUE="TRUE"/> -->
            <!-- <I PROPERTY="EQTYPE" VALUE="SteadyStokes" /> -->
            <I PROPERTY="EQTYPE" VALUE="SteadyOseen" />
	    <I PROPERTY="SolverType" VALUE="CoupledLinearisedNS_ROM" />
	    <!-- <I PROPERTY="GlobalSysSoln" VALUE="DirectFull" /> -->
            <!--  <I PROPERTY="Driver" VALUE="SteadyState" />  doing some pseudo-time integration to compute steady state-->
	    <!-- <I PROPERTY="MappingImplicitPressure" VALUE="TRUE"/>  Default = FALSE -->
	    <!-- <I PROPERTY="MappingImplicitViscous" VALUE="TRUE"/> Default = FALSE -->
            <!-- <I PROPERTY="AdvectionForm" VALUE="Convective" /> -->
            <!-- <I PROPERTY="EvolutionOperator" VALUE="Nonlinear" />  -->
            <I PROPERTY="Projection" VALUE="Galerkin" />
            <!-- <I PROPERTY="TimeIntegrationMethod" VALUE="IMEXOrder1" />-->
        </SOLVERINFO>

        <PARAMETERS>
		<!--  The updated parameters are: X_tilde = 2.01704e+07 and Delta_tilde = 2.13066e+21   -->
	    <P> ControlCoeff = 10 </P> 
	    <!-- <P> FilterWidth = 2e+1 </P> -->
            <P> TimeStep = 1e-4    </P>
            <P> TOL = .005    </P>
            <P> NumSteps = 1e6      </P>
            <P> IO_CheckSteps = 1000       </P>
            <P> IO_InfoSteps = 1000       </P>
            <P> Kinvis = 10       </P>
            <P> Kinvis_max = 123       </P>
            <P> load_snapshot_data_from_files = 1       </P>
            <P> number_of_snapshots = 4       </P>
            <P> param0 = 0.1       </P>
            <P> param1 = 0.5       </P>
            <P> param2 = 1       </P>
            <P> param3 = 10       </P>
            <P> ref_param_index = 2       </P>
            <P> ref_param_nu = 1       </P>
            <P> POD_tolerance = .9999       </P>
	    <P> globally_connected = 2      </P>
        </PARAMETERS>

        <VARIABLES>
            <V ID="0"> u </V>
            <V ID="1"> v </V>
        </VARIABLES>

        <BOUNDARYREGIONS>
            <B ID="0"> C[1] </B>
            <B ID="1"> C[2] </B>
            <B ID="2"> C[3] </B>
        </BOUNDARYREGIONS>

        <BOUNDARYCONDITIONS>
            <REGION REF="0">    <!-- Walls -->
                <D VAR="u" VALUE="0" />
                <D VAR="v" VALUE="0" />
            </REGION>
            <REGION REF="1">    <!-- InFlow -->
                <!-- <D VAR="u" VALUE="(y*(1-y))" /> -->
                <D VAR="u" VALUE="(y*(3-y))" />
                <D VAR="v" VALUE="0" />
            </REGION>
            <REGION REF="2">    <!-- OutFlow -->
                <N VAR="u" VALUE="0" />
                <N VAR="v" VALUE="0" />
            </REGION>
        </BOUNDARYCONDITIONS>

      	 <FUNCTION NAME="InitialConditions">
            <E VAR="u" VALUE="0" />
            <E VAR="v" VALUE="0" />
            <E VAR="p" VALUE="0" />
        </FUNCTION>  

	<!--  <FUNCTION NAME="InitialConditions">
		<F FILE="Test1_13_eig_0.fld" />
	</FUNCTION> -->

       <!--  <FUNCTION NAME="InitialConditions">
		<F FILE="nn.fld" />
	</FUNCTION>      -->


         <FUNCTION NAME="ExactSolution">
            <E VAR="u" VALUE="y*(1-y)" />
            <E VAR="v" VALUE="0" />
            <E VAR="p" VALUE="0" />
        </FUNCTION>  

	<FUNCTION NAME="TestSnap1">
	      <F FILE="OseenROM_sol0p1.fld" />
	</FUNCTION>  

	<FUNCTION NAME="TestSnap2">
	      <F FILE="OseenROM_sol0p5.fld" />
	</FUNCTION>  

	<FUNCTION NAME="TestSnap3">
	      <F FILE="OseenROM_sol1.fld" />
	</FUNCTION>  

	<FUNCTION NAME="TestSnap4">
	      <F FILE="OseenROM_sol10.fld" />
	</FUNCTION>  

        <FUNCTION NAME="Mapping">
            <E VAR="x" VALUE="x + sin(2*PI*t) * 0.05 * (exp(-( abs(x-0.5) + abs(y-0.5) ) ))" />
            <E VAR="y" VALUE="y" />
        </FUNCTION>
        <FUNCTION NAME="MappingVel">
            <E VAR="vx" VALUE="2*PI* cos(2*PI*t) * 0.05 * (exp(-( abs(x-0.5) + abs(y-0.5) ) ))" />
            <E VAR="vy" VALUE="0.0" />
        </FUNCTION>

 	 <FORCE TYPE="MovingBody">
		<PARAM NAME="OutputFile">DragLift.frc</PARAM>
		<PARAM NAME="OutputFrequency">10</PARAM>
		<PARAM NAME="Boundary"> B[0] </PARAM>
	</FORCE>    
                    
    <FUNCTION NAME="AdvectionVelocity">
    <!--  <F FILE="Channel_1p5_TT.fld" /> -->
      <E VAR="u" VALUE="0" />
      <E VAR="v" VALUE="0" />
    </FUNCTION> 

