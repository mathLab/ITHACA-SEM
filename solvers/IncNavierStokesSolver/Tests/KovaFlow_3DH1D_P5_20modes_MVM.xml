            <E ID="15"> 17 14   </E>
            <E ID="16"> 17 18   </E>
            <E ID="17"> 18 15   </E>
            <E ID="18"> 18 19   </E>
            <E ID="19"> 19 16   </E>
            <E ID="20"> 19 7   </E>
            <E ID="21"> 7 8   </E>
            <E ID="22"> 13 1   </E>
            <E ID="23"> 1 4   </E>
